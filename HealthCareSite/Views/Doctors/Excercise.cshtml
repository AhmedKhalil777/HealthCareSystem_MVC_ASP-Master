@model IEnumerable<HealthCareSite.Models.Exercise>

@{
    ViewBag.Title = "Excercise";
    Layout = "~/Views/Shared/_DoctorLayout.cshtml";
}

<h2>Excercise</h2>

<p>
    @Html.ActionLink("Create New", "Create", "Exercises",null, new { @class = "btn btn-success" }) )
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Media)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Body_matching)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Repeat)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Media)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Body_matching)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Repeat)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Ex_ID }, new { @class = "btn btn-warning" }) ) |
            @Html.ActionLink("Details", "Details", new { id = item.Ex_ID }, new { @class = "btn btn-info" }) ) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Ex_ID }, new { @class = "btn btn-danger" }) )
        </td>
    </tr>
}

</table>
